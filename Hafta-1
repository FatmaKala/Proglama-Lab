def bubble_sort (my_array):
    i aralığında (len (my_array) -1,0, -1):
        (i) aralığında j için:
            Eğer (my_array [j]> my_array [j + 1]):
                t = my_array [j]
                my_array [j] = my_array [j + 1]
                my_array [j + 1] = t


